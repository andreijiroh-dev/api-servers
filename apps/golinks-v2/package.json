{
	"name": "cloudflare-workers-openapi",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"types:gen": "wrangler types && prisma generate",
		"migrations:new": "wrangler d1 migrations create golinks",
		"migrations:diff": "prisma migrate diff --from-local-d1 --to-schema-datamodel ./prisma/schema.prisma",
		"migrations:apply": "wrangler d1 migrations apply golinks-db",
		"d1-exec:dev": "wrangler d1 execute golinks-db --local",
		"d1-exec:prod": "wrangler d1 execute golinks-db --remote",
		"args": "echo $*"
	},
	"dependencies": {
		"@prisma/adapter-d1": "^5.16.2",
		"@prisma/client": "^5.16.2",
		"chanfana": "^2.0.2",
		"hono": "^4.4.7",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240712.0",
		"@types/node": "20.14.10",
		"@types/service-worker-mock": "^2.0.1",
		"prisma": "^5.16.2",
		"typescript": "^5.5.3",
		"wrangler": "3.65.0"
	}
}
